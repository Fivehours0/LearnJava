<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="stu" class="LearnSpring.DICollection.Stu">
        <!--注入数组-->
        <property name="course">
            <array>
                <value>java</value>
                <value>c++</value>
            </array>
        </property>

        <!--注入List-->
        <property name="list">
            <array>
                <value>123</value>
                <value>23</value>
            </array>
        </property>

        <!--注入Map-->
        <property name="map">
            <map>
                <entry key="name" value="d"/>
                <entry key="age" value="23"/>
            </map>
        </property>

        <!--注入Set-->
        <property name="set">
            <set>
                <value>123</value>
                <value>23</value>
            </set>
        </property>

        <!--注入list，list中的数据类型为Course类-->
        <property name="courseList">
            <array>
                <!--注入的第一个类-->
                <bean class="LearnSpring.DICollection.Course">
                    <property name="name" value="java class"/>
                </bean>
                <!--注入的第二个类-->
                <bean class="LearnSpring.DICollection.Course">
                    <property name="name" value="data structure"/>
                </bean>

            </array>
        </property>



    </bean>
</beans>